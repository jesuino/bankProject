package demo.bankaccount;

rule "definirLimiteMedio"
	dialect "mvel"
	ruleflow-group "limite"
	when
		c : Cliente( salario >=2000 && salario < 5000 && limite == null)
	then
       modify( c ){
           c.setLimite( 3000 )
       }
    System.out.println("Regra definirLimiteMedio disparada! ");
end
